        </competency>
        <competency in="Java" level="skilled" experience="3 years">
            <technology name="JavaEE"/>
            <technology name="Spring Framework"/>
            <technology name="EJB"/>
            <technology name="CDI"/>
            <technology name="JPA"/>
            <technology name="WebSocket"/>
            <technology name="Servlet"/>
            <technology name="JUnit"/>
            <technology name="Maven"/>
        </competency>
        <competency in="VCS" level="basic syntax">
            <technology name="Git"/>
            <technology name="SVN"/>
        </competency>
        <competency in="MySQL" level="basic syntax"/>
        <competency in="Oracle" level="basic syntax"/>
    </competencies>
</aribal>