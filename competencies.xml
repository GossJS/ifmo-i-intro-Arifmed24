<?xml version="1.0" ?>
<!DOCTYPE aribal [
        <!ELEMENT aribal (competencies)>
        <!ELEMENT competencies (competency+)>
        <!ELEMENT competency (technology*)>
        <!ELEMENT technology EMPTY>
